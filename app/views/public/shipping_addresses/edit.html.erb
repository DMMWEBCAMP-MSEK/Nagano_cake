<div class="container mt-5">
<h3>配送先編集</h3>
<%= form_with model:@shipping_address, url: shipping_address_path, method: :patch do |f| %>
<table>
  <tr>
    <div class="form-group">
      <th><%= f.label :郵便番号 %></th>
      <td><%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %></td>
    </div>
  </tr>
  <tr>
    <div class="form-group">
      <th><%= f.label :住所 %></th>
      <td><%= f.text_field :address, autofocus: true, autocomplete: "address" %></td>
    </div>
  </tr>
  <tr>
    <div class="form-group">
      <th><%= f.label :宛名 %></th>
      <td><%= f.text_field :name, autofocus: true %></td>
    </div>
  </tr>
</table>
<div class="form-group">
  <%= f.submit "変更内容を保存" %>
</div>
<% end %>
</div>